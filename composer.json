{
  "name": "yxvt/beermission-laravel",
  "description": "Laravel bridge for the Beermission package",
  "type": "library",
  "require": {
    "php": "^7.4",
    "yxvt/beermission": "^1.0.0",
    "illuminate/container": "^8.0@dev",
    "illuminate/contracts": "^8.0@dev",
    "illuminate/database": "^8.0@dev"
  },
  "require-dev": {
    "laravel/tinker": "^2.4",
    "orchestra/testbench": "^6.0@dev",
    "phpstan/phpstan": "^0.12.0@dev",
    "phpunit/phpunit": "^9.4@dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Bartek Stecko",
      "email": "stecko.bartosz@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Yxvt\\BeermissionLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yxvt\\BeermissionLaravel\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yxvt\\BeermissionLaravel\\BermissionServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
